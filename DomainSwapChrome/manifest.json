{
   "background": { "scripts": [ "background.js" ] },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_popup": "popup.html"
   },
   "description": "Swaps/replaces domains in links for a different domain.",
   "homepage_url": "https://github.com/FtrSpec/DomainSwap",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Domain Swap",
   "options_ui": { "page": "popup.html" },
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "storage", "<all_urls>" ],
   "short_name": "Hidden",
   "version": "0.7",
   "web_accessible_resources": [ "Screenshot.png" ]
}
